<!doctype html>
<html>
	<head>
		<title>Articles</title>
	</head>
	<body>
		<h2>A List of Stuff that is Wanted</h2>
		<ul>
		<% article.forEach(function(article) { %>
		<li> <%= article.title %> </li>
		<li> <%= article.date %> </li>
		<li> <%= article.summary %> </li>
	<% }) %>
		</ul>
	</body>
</html>